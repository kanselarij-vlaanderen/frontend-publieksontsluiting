{{! template-lint-disable }}
{{! Not octanified }}

{{#if (not await dateObjectsToEnable)}}
  {{ember-flatpickr
    classNames="vl-input-field vl-input-field--block"
    locale="nl"
    enableTime=enableTime
    time_24hr=true
    date=(readonly selectedDate)
    onChange=(action "dateChanged")
    getFlatpickrRef=(action (mut flatpickrRef))
    dateFormat=dateFormat
  }}
{{else}}
  {{ember-flatpickr
    classNames="vl-input-field vl-input-field--block"
    locale="nl"
    enable=datesToEnable
    enableTime=enableTime
    time_24hr=true
    date=(readonly selectedDate)
    onChange=(action "dateChanged")
    getFlatpickrRef=(action (mut flatpickrRef))
    dateFormat=dateFormat
  }}
{{/if}}